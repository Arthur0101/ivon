$(function() {
    //MENU
    // $(".nav li").hover(function() {
    //     $(this).toggleClass("active");
    // })
    $(".menu").click(function() {
        $("body").toggleClass("actMenu");
    })
    //fixed
    $(window).scroll(function(){
        // 設變數WRAP
        var WRAP = $("#wrap");
        if($(window).width()){
            WRAP.addClass("fixed");
        }else{
            WRAP.removeClass("fixed");
        }
    })
})