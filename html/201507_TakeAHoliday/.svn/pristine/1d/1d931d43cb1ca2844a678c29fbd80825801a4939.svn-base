html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video { margin: 0; padding: 0; border: 0; font: inherit; font-size: 100%; vertical-align: baseline; }

html { line-height: 1; }

ol, ul { list-style: none; }

table { border-collapse: collapse; border-spacing: 0; }

caption, th, td { text-align: left; font-weight: normal; vertical-align: middle; }

q, blockquote { quotes: none; }
q:before, q:after, blockquote:before, blockquote:after { content: ""; content: none; }

a img { border: none; }

article, aside, details, figcaption, figure, footer, header, hgroup, main, menu, nav, section, summary { display: block; }

img { display: block; }

.menu span, #topBt { display: block; overflow: hidden; text-indent: 100%; white-space: nowrap; }

main { max-width: 100%; margin-left: auto; margin-right: auto; max-width: 960px; }
main:after { content: " "; display: block; clear: both; }

html *, html *:before, html *:after { font-size: 14px; line-height: 20px; color: #000; font-family: "Century Gothic", "Microsoft JhengHei", "Apple LiGothic Medium", "PMingLiU", "sans-serif", "serif"; }

html, body { width: 100%; height: 100%; min-width: 960px \9; background-color: #f1ece6; }

body { margin: 0; padding: 35px 0 0 0; }

#wrap { background: url(../images/bg-sea.jpg) no-repeat center top; padding-bottom: 20px; padding-top: 70px; }

.menu { width: 30px; height: 28px; padding: 10px 5px 0 10px; background-color: #c8e02a; border: 2px solid #fff; -moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius: 4px; position: fixed; z-index: 20; top: 20px; left: 20px; display: none; }
.menu:before, .menu:after { content: ""; display: block; width: 25px; height: 3px; background-color: #fff; position: absolute; }
.menu:after { top: 24px; }
.menu span { background-color: #fff; width: 25px; height: 3px; position: absolute; top: 17px; }

nav { width: 100%; position: fixed; top: 35px; left: 0; z-index: 20; height: 70px; background-color: #c8e02a; }
nav ul { text-align: center; }
nav li { display: inline-block; line-height: 70px; -moz-border-radius: 20px; -webkit-border-radius: 20px; border-radius: 20px; -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box; position: relative; }
nav li:hover ul { display: block; background-color: none; }
nav li.on > a { border: 2px solid #222; }
nav li.on > a:hover { border: 2px solid #222; }
nav li a { padding: 0 20px; display: inline-block; font-size: 18px; line-height: 30px; text-decoration: none; font-weight: bolder; border: 2px solid #c8e02a; -moz-border-radius: 20px; -webkit-border-radius: 20px; border-radius: 20px; -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box; }
nav li a:hover { border: 2px solid #222; -moz-border-radius: 20px; -webkit-border-radius: 20px; border-radius: 20px; -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box; }
nav li ul { position: absolute; top: 60px; left: -28px; width: 170px; background-color: #fff; -moz-border-radius: 10px; -webkit-border-radius: 10px; border-radius: 10px; display: none; }
nav li ul:before { content: ""; display: block; position: absolute; top: -10px; left: 80px; width: 0; height: 0; border-style: solid; border-width: 0 6px 10px 6px; border-color: transparent transparent #fff transparent; }
nav li ul li { line-height: 10px; }
nav li ul li + li { border-top: 1px solid #c8e02a; -moz-border-radius: 0; -webkit-border-radius: 0; border-radius: 0; }
nav li ul li a { font-size: 14px; line-height: 30px; width: 170px; display: inline-block; border: none; -moz-border-radius: 0; -webkit-border-radius: 0; border-radius: 0; }
nav li ul li a:hover { border: none; }

.week a { background-color: #dc3b31; color: #c8e02a; border: 2px solid #dc3b31; }
.week a img { float: right; }
.week a:hover { border: 2px solid #dc3b31; }
.week strong { font-size: 22px; line-height: 30px; color: #c8e02a; font-weight: bolder; }
.week.on a, .week.on a:hover { border-color: #dc3b31; }

#topBt { position: fixed; right: 20px; bottom: -50px; display: block; width: 50px; height: 50px; z-index: 50; -moz-border-radius: 50%; -webkit-border-radius: 50%; border-radius: 50%; text-decoration: none; background: #000; -moz-transition: bottom, 1s; -o-transition: bottom, 1s; -webkit-transition: bottom, 1s; transition: bottom, 1s; }
#topBt:before, #topBt:after { content: ""; display: block; width: 0; height: 0; border-style: solid; border-width: 0 12px 16px 12px; position: absolute; left: 13px; }
#topBt:before { border-color: transparent transparent #fff transparent; top: 14px; }
#topBt:after { border-color: transparent transparent #000 transparent; top: 18px; }

.fixed #topBt { bottom: 20px; }

@media (min-width: 0px) { html, body { min-width: 320px; } }
@media (max-width: 980px) { body { padding-top: 0 !important; }
  #lionhead, #lionfoorter { display: none !important; }
  #wrap { padding-top: 0; background-size: cover; background-position: center top; }
  nav { width: 180px; height: 100%; position: fixed; top: 0; left: 0; display: none; }
  nav li { line-height: 50px; width: 100%; -moz-border-radius: 0; -webkit-border-radius: 0; border-radius: 0; }
  nav li + li { border-top: 1px solid #fff; }
  nav li.on { background-color: #333; }
  nav li.on > a { border: none; color: #fff; }
  nav li.on > a:hover { border: none; }
  nav li a { display: inline-block; width: 100%; border: none; padding: 0; }
  nav li a:hover { border: none; }
  nav li ul { position: relative; top: 0; left: 0; margin: auto; margin-bottom: 10px; display: block; }
  nav li ul li { border: none; }
  nav li ul li a { line-height: 50px; }
  nav li ul li + li { margin-bottom: 0; }
  .menu { display: block; }
  .week { margin-top: 10px; border: none; }
  .week a { width: 170px; position: relative; text-indent: -20px; line-height: 50px; }
  .week a img { position: absolute; top: 10px; right: 20px; }
  .actMenu { overflow: hidden; }
  .actMenu nav { display: block; overflow-y: auto; overflow-x: hidden; }
  .actMenu #wrap { position: relative; left: 180px; }
  .actMenu .menu { left: 190px; }
  .actMenu .menu:before { -moz-transform: rotate(-45deg); -ms-transform: rotate(-45deg); -webkit-transform: rotate(-45deg); transform: rotate(-45deg); margin-top: 0px; top: 18px; }
  .actMenu .menu:after { -moz-transform: rotate(45deg); -ms-transform: rotate(45deg); -webkit-transform: rotate(45deg); transform: rotate(45deg); top: 18px; }
  .actMenu .menu span { display: none; } }
@media (max-width: 720px) { #wrap { background-size: contain; } }

